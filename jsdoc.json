{
  "tags": {
    "allowUnknownTags": true
  },
  "source": {
    "include": ["./src/js"],
    "includePattern": ".+\\.js(doc)?$",
    "excludePattern": "docs|lib"
  },
  "plugins": [],
  "templates": {
    "applicationName": "FooTable V3",
    "default":{
      "outputSourceFiles": false
    },
    "cleverLinks": false,
    "monospaceLinks": true
  },
  "opts": {
    "destination": "docs/jsdocs",
    "readme": "README.md",
    "mainpagetitle": "FooTable Developer Documentation",
    "recurse": true,
    "private": true
  }
}